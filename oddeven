import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int[] num={1,2,3,4,5,6,8};
	    int a=0;
	    int b=0;
	    for (int i:num) {
	        if(i%2==0){
	            a++;
	           } else{
	                b++;
	           }}
	                System.out.println("Even Number:"+a);
	           
	                System.out.print("Odd Numbers:"+b);
	            }
	        }
